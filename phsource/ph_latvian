
phoneme @
  vowel starttype (@) endtype (@)
  length 120
  formants vowel/8_7
endphoneme
 
phoneme i
  vowel starttype (i) endtype (i)
  length 110
  formants vowel/i_6
  linkout ;
endphoneme

phoneme i:
  vowel starttype (i) endtype (i)
  length 300
  formants vowel/i_6
  linkout ;
endphoneme


