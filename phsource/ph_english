  vowel  starttype (a)   endtype (@)
  length 270
  formants vwl_en/aU@
  linkout r-
endphoneme


// extra vowels

phoneme e:
  vowel  starttype (e)  endtype (e)
  length 210
  formants vowel/e
endphoneme


