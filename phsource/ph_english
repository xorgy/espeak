  vowel  starttype (u)  endtype (@)
  length 200
  formants vdiph2/uu@
  linkout r-
endphoneme


phoneme aI@
  vowel  starttype (a)  endtype (@)
  length 270
  formants vwl_en/aI@
  linkout r-
endphoneme


