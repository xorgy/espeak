 
phoneme i
  vowel starttype (i) endtype (i)
  length 110
  formants vowel/ii_3
  linkout ;
endphoneme

phoneme i:
  vowel starttype (i) endtype (i)
  length 260
  formants vowel/i_3
  linkout ;
endphoneme

