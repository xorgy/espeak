phoneme l
  liquid
  length 100
  lengthmod 7
  beforenotvowel l/2       // use 'dark' [l] after a vowel
  formants l/l
  after _ l/_l  t l/tl
  before _ l/l_2
endphoneme

phoneme w
  liquid
  length 100
  beforenotvowel w/
  lengthmod 7
  formants w/w2
  after _  w/w2
endphoneme


