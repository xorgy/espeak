// function words

den	dEn	$u
en	e:n	$u
et	ed	$u

// conjunctions
og	V	$u+ $pause  // and
men		$pause	// but
eller		$u+ $pause $only	// or
mens		$pause	// while
for	fV
fordi	fVd'i $pause	// because - PB: changed stress
når	nVr		$pause	// while
som		$u+ $pause   // who (relative)
der	d&r	$u+ $pause   // PB changed from [d]A
hvor	v'Vr	// where
hvorfor	vVf'Vr	// why

// prepositions
på		pO:
til			$u+	// toward
fra			$u+	// from
ved			$u+	// at
om			$u+ $brk	// about, within
med			$u+	// with
af      &  	// PB changed from [a?] to [&] - sound too short in a sentence
            // Also removed $u from 'af' and other words. They disappeared.
ad      &:  // PB added 'ad'
at		&: $u // PB changed from [at] to [&:]
hos			// at - PB: removed [$u+]
som	sVm	    // which / that - PB: removed [$u+]

// pronouns
jeg	jai	$u+	// I
du du $u+
de	di	$u
han	$u
hun	$u
det	de $u
vi	$u
i	_i: $u+
mig	maj	$u	// me
dig	daj	$u
dem	$u
ham	$u
hende	$u
sig	saj
os	Vs $u
jer
denne	$u+	// this
dette	$u+
disse	$u+	// these
hvis ves   $u+  // PB - whose

// possessive pronouns
