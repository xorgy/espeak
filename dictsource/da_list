_smc	sm'O:lk,ap
_rev	riv'3:sd
_tur	t'3:nd
_bar	_b'A@
_rfx	rtroflEks
_crl	k'3:l
_hok	h'uk


// character names
_cap	stoVt boustaw
_?A	boustaw
_??	symboL
_#9	tab
_#32	blank

