%	pRos'end	$max3
@	sn,&:bel'&:
/	sgRVsdrai	$max3
\	'Vmvendsgr,VsdRai $max3
_,	k'oma:
_;	se:mi:ko:lVn
_:	k'o:lVn
_.	puNtum
_?	spWrgsmVlstajn
_!	udRVbstejn
_-	binnesdrai
__	b'Onsdrai
