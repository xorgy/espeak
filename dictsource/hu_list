// Articles
a         $u $strend2   // the
az       $u $strend2
//aki       $u+   // that

ez        $unstressend   // this


// Pronouns

// If these are only used for emphasis then then don't need $u
én        $unstressend
te        $unstressend

ők	$unstressend $pause
mi        $unstressend
ti        $unstressend
ők        $unstressend
maga     $unstressend
maguk     $unstressend


// Conjunctions
