// prepositions
i	i: $u $brk	// in
på	$u		// on, in, at
vid	$u $brk		// by, at, next to
av	$u		// of, by
med	$u $brk		// with
för	$u $brk

om	$u $brk		// about
till	$u $brk		// to
från	fr'o:n $u $brk		// from
mot	$u $brk		// towards
över	$brk		// over
längs	$brk		// along
genom	$brk		// through
efter	$brk		// after
innan	$brk		// before
under	$brk		// during


// conjunctions
och	'Ok $u $pause	// and
eller	$u $pause	// or
om	$u $pause	// if
då	$u $brk		// as
fastän	$pause		// although
medan	$pause		// while

än	En $u		// than
att	$u $pause	// that
som	$u $pause	// that, which

// negative

// questions


// auxillary verbs  (be, have, can, etc)
är	'E:r $u		// is
var	vA:r $u		// was 

har	$u		// has
hade	hadE $u		// had

ska	skA: $u		// will (future)

kan	k'an $u		// can
kunde	$u		// could
vill	$u		// want to
ville	$u		// wanted to
ska	skA: $u		// should, supposed to
skulle	$u
bör	$u		// should, ought to
borde	$u
brukar	$u		// usually
brukade	$u		// used to


// some common adjectives and adverbs
någon	 n,o:gOn $u		// some
något	 n,o:gOt $u
några	  n'o:gra $u


