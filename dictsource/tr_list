// function words

// conjunctions
ve	$brk	// and
yoksa	$brk	// or
veya	$brk	// or
ama	$brk $1	// but
ki	$brk	// that

mı	$u	// question
mi	$u
mu	$u
mü	$u


// pronouns
bu	$u+	// this
şu	$u+	// that

ben	$u+	// I
sen	$u+	// you
o	$u+	// it
siz	$u+	// we
biz	$u+	// you



bir	$u+	// a (or one)

da	$u	// also
de	$u
ta	$u
te	$u

ile	$u


// EXCEPTIONS

// Person Names

// Place Names
ankara		$1
istanbul	$2

