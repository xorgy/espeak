// translation rules for Romanian
// This file is UTF-8 encoded



.group a
        a        a
        ai       aI
        ai (_    'aI
        au       aU   // ?? only at end of word ??
        au (_    'aU
        a (AAA   a
     &) a (_S1q  a
    gi) a (_S1q  'a
    V@) a (_     'a	
    ăi) a (_     'a
     @) abil (_  'abil
     _) anti (@P4  anti
     @) ale (_   'ale

.group ă
        ă        @
        ăi       @I
        ău       @U  // ?? only at end of word ??
        ău (_    @U

        ăi (m_   @'i
        ăi (t_   @'i
        ăi (ţi_  @'i

     &) ă (_S1   @
        ări (A_  @*'i
        ării (_  @*'iI
        ări (AA_ @*'i

.group â
        â        y
        âi       yI
        âu       yU


.group b
        b        b
     @) b (i_    b;

     @) b (il_   =b
    mo) bil (_   b'il


.group c
        c        k
        c (e     tS
        c (i     tS
        ci (o    tS
        cii      tSiI
        ch       k
    @@) col (_   =kol
    @@) coli (_S1t  =I^
    @@) cole (_S1t  =e

.group d
        d        d
     @) d (i_    d;


.group e
        e        e
        ei       eI
//        eu       eU   // only at end of word ??
//        eu       e[u    // ??
        eu (_    'eU
        ea       ea
     &) ea (_S2q ea
     &) eSi      eaj
        eau      eaw
        eoa      'eOa
        eo       eo
        e (AAA   e

     @) eş (_S2t eS
// &) es (_S1q e
    &C) e (re_   %e
 
   _ac) ee (a	 eI

     r) ei (er_  'eI

     @) e (re_   =e
     @) e (ri_   =e
     @) e (rea_  =e
     i) e (re_   e
     i) e (ri_   e
     i) e (rea_  e


.group f
        f        f
     @) f (i_    f;


.group g
        g        g
        g (e     dZ
        g (i     dZ
        gii      dZiI
        gh       g


.group h
        h        h
     @) h (i_    C


.group i
        i        i
        i (A     j
    &C) i (_N$1q i/
     &) i (_N$1q i
     &) i (-N$1q i        // don't reduce to [I^] if a hyphen follows
    &') i (_     i
   ţi-) i (_     _        // combined with previous word as [iI]
    Cr) i (_     i
    V&) i (_     'i       // verb
    ăC) ia (_    'ia
    âC) ia (_    'ia
    ăC) ie (_    'ie
    âC) ie (_    'ie
     @) ie (_S1q    =ie
        iei (_   'ieI
        ii       iI
     &) ii (_S2q iI
        iii      iji
        iu (_    iU
        iu       ju

     &) ic (_    =ik
     &) ice (_   =itSe
     &) icile (_  =itSile  // ??
     &) icilor (_ =itSilor // ??
     &) ing (_   %iNg     // foreign words
        i (t_    'i

.group î
        î        y


.group j
        j        Z
     @) j (i_    Z;


.group k
        k        k


.group l
        l        l
     @) l (i_    l;
     _) l (_     le
     -) l (_     l         // -l

    iu) l (_S1t   l
     &) le (_S2t  le
     &) lor (_S3t lor
     &) lui (_S3t luI
     &) logi (A_  lodZ'i
     &) logii (_  lodZ'iI

.group m
     _) m (_     me
        m        m
     @) m (i_    m;
     _) m (-a    m
@-_) mi  mI^                // ??


.group n
     _) n (_     ne
        n        n
     @) n (i_    n;
        n (g     N
        n (k     N
     @) n (tic_  =n
     @) n (ic_   =n
     _) n (-a     n


