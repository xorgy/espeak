        qu       kw

.group r
        r        r
        rr       R
     K) r        @-*
     A) r (A     *
     r) r
        rs (_    S

.group s
        s        s
        ss       ss
        ss (_    s
        sch      S
        sj       S
        skj      S
        sk (i    S
        sk (y    S
        sk (ei   S
        sk (øy   S
     r) s        S
        s (l     S
     A) s (lA    s 

.group t
        t        t
        tt       t:
        ti (on   S
        tj       s;

 

.group u
        u        u-:
        u (C%    u-
     X) u (CCK   u-
        ui       u-I

